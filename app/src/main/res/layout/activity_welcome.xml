<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.ls.kylibary.main.WelcomeActivity">

    <ImageView
        android:id="@+id/sc"
        android:background="@drawable/wel"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.ls.libarys.view.CountDownView
        android:id="@+id/cdv_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|right"
        android:layout_marginTop="40dp"
        android:layout_marginRight="20dp"
        app:cd_animator_time="3"
        app:cd_animator_time_unit="ç§’"
        app:cd_arc_color="@color/material_red"
        app:cd_arc_width="2dp"
        app:cd_bg_color="#00000000"
        app:cd_circle_radius="20dp"
        app:cd_location="top"
        app:cd_retreat_type="forward"
        app:cd_text_color="@color/white"
        app:cd_text_size="9sp" />
</FrameLayout>
